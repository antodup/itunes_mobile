  pod 'Alamofire', '~> 5.0.0-beta.3'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'


  target 'ItunesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ItunesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
